{
  "private": true,
  "name": "blockchain-wallet-v4",
  "version": "0.1.0",
  "scripts": {
    "start": "cd packages/blockchain-wallet-v4-frontend && npm run start",
    "start:prod": "cd packages/blockchain-wallet-v4-frontend && npm run start:prod",
