{
  "name": "blockchain-wallet-v4-frontend",
  "version": "0.0.1",
  "description": "Wallet user interface",
  "keywords": [
    "blockchain",
    "react",
    "wallet"
  ],
  "homepage": "https://github.com/blockchain/blockchain-wallet-v4-frontend#readme",
  "bugs": {
    "url": "https://github.com/blockchain/blockchain-wallet-v4-frontend/issues"
  },
  "license": "ISC",
  "author": {
    "name": "(Dream Team) Blockchain.info",
    "url": "https://blockchain.info"
  },
  "contributors": [
    "Justin Tormey",
    "Jaume Pernas",
    "Guillaume Marquilly"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockchain/blockchain-wallet-v4-frontend.git"
  },
  "scripts": {
    "build": "lerna bootstrap && webpack --config webpack.config.js --progress --colors",
    "build:prod": "lerna bootstrap && NODE_ENV=production BABEL_ENV=production webpack -p --config webpack.config.js --progress --colors",
    "coverage": "./../../node_modules/.bin/jest --coverage",
    "generate": "babel-node ./tools/generateDefaultLocale.js",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "start:prod": "webpack-dev-server -p --config webpack.config.js --progress --colors --watch",
    "test": "./../../node_modules/.bin/jest",
    "test:debug": "node --inspect-brk ./../../node_modules/.bin/jest --runInBand",
    "test:watch": "./../../node_modules/.bin/jest --watchAll"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/../../coverage/blockchain-wallet-v4-frontend",
    "coverageReporters": [
      "json",
      "html"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/../../node_modules/identity-obj-proxy",
      "^locales(.*)$": "<rootDir>/src/assets/locales/$1"
    },
    "setupFiles": [
      "<rootDir>/../../jest.shim.js",
      "<rootDir>/../../jest.config.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/../../node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.js$": "<rootDir>/../../node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(blockchain-info-components|blockchain-wallet-v4)/)"
    ]
  },
  "dependencies": {
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-polyfill": "^6.23.0",
    "base-64": "^0.1.0",
    "bignumber.js": "^5.0.0",
    "bip21": "^2.0.1",
    "bip39": "^2.3.1",
    "bitcoin-sfox-client": "^0.2.6",
    "bitcoinjs-lib": "^3.1.1",
    "blockchain-info-components": "^0.0.1",
    "blockchain-wallet-v4": "^1.0.1",
    "connected-react-router": "^4.2.3",
    "data.either": "~1.5.1",
    "data.maybe": "^1.2.2",
    "es6-promise": "^4.1.0",
    "fast-memoize": "^2.2.8",
    "libphonenumber-js": "^0.4.40",
    "moment": "^2.19.2",
    "prop-types": "^15.6.0",
    "qrcode.react": "^0.7.2",
    "ramda": "^0.25",
    "react": "^16.1.1",
    "react-animations": "^1.0.0",
    "react-browser-detection": "^1.0.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-csv-2": "^1.0.9",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.8",
    "react-highcharts": "^13.0.0",
    "react-intl": "^2.3.0",
    "react-intl-tel-input": "^5.0.1",
    "react-onclickoutside": "^6.7.1",
    "react-qr-reader": "^2.0.0",
    "react-redux": "^5.0.5",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^2.2.1",
    "read-blob": "^1.1.0",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-saga": "^0.16.0",
    "redux-ui": "^0.1.1",
    "styled-components": "^2.2.3",
    "webpack-bundle-analyzer": "^2.9.1",
    "zxcvbn": "^4.4.2"
  },
  "standard": {
    "globals": [
      "jest",
      "test",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  }
}
